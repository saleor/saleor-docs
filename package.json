{
  "description": "Saleor e-commerce documentation.",
  "repository": {
    "type": "git",
    "url": "https://github.com/saleor/saleor-docs.git"
  },
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=20 <=21",
    "npm": ">=10"
  },
  "scripts": {
    "start": "docusaurus start",
    "dev": "npm run start",
    "build": "docusaurus build",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "publish-gh-pages": "docusaurus deploy",
    "version": "docusaurus version",
    "rename-version": "docusaurus rename-version",
    "prepare": "husky",
    "swizzle": "docusaurus swizzle",
    "create-storefront-schema": "node ./scripts/create-storefront-schema.js",
    "generate-api-reference": "docusaurus graphql-to-doc --force --pretty --root './.tmp'",
    "highlight-api-reference": "node ./scripts/highlight.js",
    "inject-examples": "node ./scripts/inject-examples.js",
    "move-generated-reference": "rimraf docs/api-storefront docs/api-reference && mv .tmp/* docs/",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "update-api-reference": "npm run generate-api-reference && npm run highlight-api-reference && npm run move-generated-reference && npm run inject-examples"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.4.0",
    "@docusaurus/preset-classic": "^3.4.0",
    "@docusaurus/theme-classic": "^3.4.0",
    "@docusaurus/theme-mermaid": "^3.4.0",
    "@docusaurus/types": "^3.4.0",
    "@graphql-markdown/diff": "^1.1.4",
    "@graphql-markdown/docusaurus": "^1.24.0",
    "@graphql-tools/graphql-file-loader": "^8.0.1",
    "@types/react": "^18.2.74",
    "dotenv": "^16.4.5",
    "esbuild": "^0.23.0",
    "esbuild-loader": "^4.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-react": "^7.37.1",
    "fast-glob": "^3.3.2",
    "graphql": "^16.8.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "markprompt": "^0.1.7",
    "prettier": "^2.8.8",
    "react-markdown": "^9.0.1",
    "remark-gfm": "^4.0.0",
    "remark-lint-heading-capitalization": "^1.1.0",
    "rimraf": "^5.0.5",
    "uuid": "^9.0.1"
  },
  "dependencies": {
    "@sentry/react": "8.33.1",
    "@sentry/tracing": "7.109.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "lucide-react": "^0.407.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5"
  }
}
